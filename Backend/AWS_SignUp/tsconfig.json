{
  "compilerOptions": {
    "target": "ES2020",
    "module": "commonjs",
    "lib": [
      "es2020",
      "dom"
    ],
    "outDir": "./dist",
    "declaration": true,
    "strict": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "noImplicitThis": true,
    "alwaysStrict": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": false,
    "inlineSourceMap": true,
    "inlineSources": true,
    "experimentalDecorators": true,
    "strictPropertyInitialization": false,
    "typeRoots": [
      "./node_modules/@types"
    ],
    "types": [
      "node",
      "jest",
      "aws-lambda",
      "babel__core",
      "babel__generator",
      "babel__template",
      "babel__traverse",
      "graceful-fs",
      "istanbul-lib-coverage",
      "istanbul-lib-report",
      "istanbul-reports",
      "stack-utils",
      "yargs",
      "yargs-parser"
    ]
  },
  "exclude": [
    "node_modules",
    "cdk.out"
  ],
  "include": [
    "src/lambda/Singup/*"
  ]
}
